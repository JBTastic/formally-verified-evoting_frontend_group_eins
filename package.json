    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.8",
    "google-protobuf": "^3.21.4",
    "grpc-web": "^1.5.0",
    "protobufjs": "^7.5.3",
    "protoc-gen-grpc-web": "^1.5.0",
    "protoc-gen-js": "^3.21.4-4",
    "sanitize-html": "^2.17.0",
    "webpack-cli": "6.0.1"
  }
}
